{{#if rows}}
{{#each rows}}


    <div class="card blue-grey darken-1">
      <div class="card-content white-text">
        <span class="card-title">Subject:{{subject}}</span>
        <p>Message:{{body}}</p>
        <p>        {{body}}</p>
      </div>
      <div class="card-action">
        <a href="#">REPLY</a>
        <a href="#">DELETE</a>
      </div>
    </div>

{{/each}}

{{else}}
<div class="card blue-grey darken-1">
</div>

{{/if}}
